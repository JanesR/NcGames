	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
	''																		, ; //X2_UNICO
	'E'																		, ; //X2_MODOEMP
	'E'																		, ; //X2_MODOUN
	0																		} ) //X2_MODULO

//
// Tabela PZ5
//
aAdd( aSX2, { ;
	'PZ5'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	'PZ5'+cEmpr																, ; //X2_ARQUIVO
	'CONTROLE DE REPASSE CLIENTE'											, ; //X2_NOME
	'CONTROLE DE REPASSE CLIENTE'											, ; //X2_NOMESPA
