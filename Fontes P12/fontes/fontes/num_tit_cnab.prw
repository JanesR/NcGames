User Function NUMTITCNAB (CPREF,CTIT,CPARC)

LOCAL CNUM 		:= CTIT
LOCAL CPREFIXO	:= CPREF
LOCAL CPARCELA	:= CPARC


IF LEN(ALLTRIM(CNUM))== 9
	CNUM := SUBSTR(CNUM,4,6)
ENDIF


RETURN CPREFIXO+ALLTRIM(CNUM)+CPARCELA	  


