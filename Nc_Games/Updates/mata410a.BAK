			Endif
		Case cTab == "SD1"
			If SF1->(FieldPos("F1_PROVENT")) > 0 .And. SD1->(FieldPos("D1_PROVENT")) > 0
				If Type("M->F1_PROVENT") == "C"
					cProv := M->F1_PROVENT
