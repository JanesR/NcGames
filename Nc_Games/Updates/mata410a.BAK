Local cAtuEst  := ""											// Conteudo de retorno

DEFAULT lTOK := .F.

If !aCols[n][Len(aCols[n])] .and. lLiqProd 

	If lTOK
		cAtuEst := Posicione("SF4",1,xFilial("SF4")+aCols[n][nPosTES],"F4_ESTOQUE")
	Else
