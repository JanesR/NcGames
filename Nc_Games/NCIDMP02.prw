User Function NCIDMP02()

Local aArea := GetArea()

	Local cNomAlias := "PZW" 
	
	chkFile(cNomAlias)
	dbSelectArea(cNomAlias)
	//indices
	dbSetOrder(1)
	axCadastro(cNomAlias, "Cadastro De/Para filiais Datamace x Protheus")

RestArea(aArea)	
return



