Local lAlt      := .F.
Local lDelInd   := .F.
Local nI        := 0
Local nJ        := 0

cTexto  += "Inicio da Atualizacao" + " SIX" + CRLF + CRLF

aEstrut := { "INDICE" , "ORDEM" , "CHAVE", "DESCRICAO", "DESCSPA"  , ;
             "DESCENG", "PROPRI", "F3"   , "NICKNAME" , "SHOWPESQ" }

//
// Tabela P09
//
aAdd( aSIX, { ;
	'P09'																	, ; //INDICE
	'5'																		, ; //ORDEM
	'P09_FILIAL+P09_TIPO+P09_USER+P09_CANAL'								, ; //CHAVE
	'Tipo+Usuario Prot+Canal Venda'											, ; //DESCRICAO
	'Tipo+Usuario Prot+Canal Venda'											, ; //DESCSPA
	'Tipo+Usuario Prot+Canal Venda'											, ; //DESCENG
	'U'																		, ; //PROPRI
	''																		, ; //F3
	''																		, ; //NICKNAME
	'S'																		} ) //SHOWPESQ

//
// Tabela P0B
//
aAdd( aSIX, { ;
	'P0B'																	, ; //INDICE
	'3'																		, ; //ORDEM
	'P0B_FILIAL+P0B_PEDIDO+P0B_NUM'											, ; //CHAVE
	'Num. Pedido+Documento'													, ; //DESCRICAO
	'Num. Pedido+Documento'													, ; //DESCSPA
	'Num. Pedido+Documento'													, ; //DESCENG
	'U'																		, ; //PROPRI
	''																		, ; //F3
	''																		, ; //NICKNAME
	'S'																		} ) //SHOWPESQ

aAdd( aSIX, { ;
	'P0B'																	, ; //INDICE
	'4'																		, ; //ORDEM
	'P0B_FILIAL+P0B_EMIPED+P0B_PEDIDO'										, ; //CHAVE
	'Emis. Pedido+Num. Pedido'												, ; //DESCRICAO
	'Emis. Pedido+Num. Pedido'												, ; //DESCSPA
	'Emis. Pedido+Num. Pedido'												, ; //DESCENG
	'U'																		, ; //PROPRI
	''																		, ; //F3
	''																		, ; //NICKNAME
	'S'																		} ) //SHOWPESQ

aAdd( aSIX, { ;
	'P0B'																	, ; //INDICE
	'5'																		, ; //ORDEM
	'P0B_FILIAL+P0B_CODCLI+P0B_LOJA+P0B_PEDIDO+P0B_NIVEL'					, ; //CHAVE
	'Cod. Cliente+Loja Cliente+Num. Pedido'									, ; //DESCRICAO
	'Cod. Cliente+Loja Cliente+Num. Pedido'									, ; //DESCSPA
	'Cod. Cliente+Loja Cliente+Num. Pedido'									, ; //DESCENG
	'U'																		, ; //PROPRI
	''																		, ; //F3
	''																		, ; //NICKNAME
	'S'																		} ) //SHOWPESQ

//
// Atualizando dicion·rio
//
oProcess:SetRegua2( Len( aSIX ) )

dbSelectArea( "SIX" )
SIX->( dbSetOrder( 1 ) )

For nI := 1 To Len( aSIX )

	lAlt    := .F.
	lDelInd := .F.

	If !SIX->( dbSeek( aSIX[nI][1] + aSIX[nI][2] ) )
		cTexto += "Õndice criado " + aSIX[nI][1] + "/" + aSIX[nI][2] + " - " + aSIX[nI][3] + CRLF
	Else
		lAlt := .T.
		aAdd( aArqUpd, aSIX[nI][1] )
		If !StrTran( Upper( AllTrim( CHAVE )       ), " ", "") == ;
		    StrTran( Upper( AllTrim( aSIX[nI][3] ) ), " ", "" )
			cTexto += "Chave do Ìndice alterado " + aSIX[nI][1] + "/" + aSIX[nI][2] + " - " + aSIX[nI][3] + CRLF
