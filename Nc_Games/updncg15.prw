	aAdd(alSXB,	{	'ZAJNUM'		,'2'		,'01'			,'01'			,'Num Alt+cli.Fat+lj Fat.'			,'Num Alt+cli.Fat+lj Fat.'			,'Num Alt+cli.Fat+lj Fat.'		,''                                                                                                                                                                                                                                                          			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJNUM'		,'4'		,'01'			,'01'			,'Num Alt.Ped.'        				,'Num Alt.Ped.'        				,'Num Alt.Ped.'        				,'ZAJ_NUMALT'                                                                                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJNUM'		,'4'		,'01'			,'02'			,'Cli faturame'        				,'Cli faturame'        				,'Cli faturame'        				,'ZAJ_CLIFAT'                                                                                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJNUM'		,'4'		,'01'			,'03'			,'Loj faturame'        				,'Loj faturame'        				,'Loj faturame'        				,'ZAJ_LJFAT'                                                                                                                                                                                                                                                 			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJNUM'		,'4'		,'01'			,'04'			,'Nome Cliente'        				,'Nome Cliente'        				,'Nome Cliente'        				,'Posicione("SA1",1,xFilial("SA1")+ZAJ->ZAJ_CLIFAT+ZAJ->ZAJ_LJFAT,"A1_NOME")'                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJNUM'		,'5'		,'01'			,''  			,''                    				,''                    				,''                    				,'ZAJ->ZAJ_NUMALT'                                                                                                                                                                                                                                           			,''                                                                                                                                                                                                                                                          				})

	aAdd(alSXB,	{	'ZAJCLI'		,'1'		,'01'			,'DB'			,'Cliente Alteracao Pd'				,'Cliente Alteracao Pd'				,'Cliente Alteracao Pd'				,'ZAJ'                                                                                                                                                                                                                                                       			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJCLI'		,'2'		,'01'			,'01'			,'Num Alt+cli.Fat+lj Fat.'			,'Num Alt+cli.Fat+lj Fat.'	   		,'Num Alt+cli.Fat+lj Fat.'		,''                                                                                                                                                                                                                                                          			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJCLI'		,'4'		,'01'			,'01'			,'Num Alt.Ped.'        				,'Num Alt.Ped.'        				,'Num Alt.Ped.'        				,'ZAJ_NUMALT'                                                                                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJCLI'		,'4'		,'01'			,'02'			,'Cli faturame'        				,'Cli faturame'        				,'Cli faturame'        				,'ZAJ_CLIFAT'                                                                                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJCLI'		,'4'		,'01'			,'03'			,'Loj faturame'        				,'Loj faturame'        				,'Loj faturame'        				,'ZAJ_LJFAT'                                                                                                                                                                                                                                                 			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJCLI'		,'4'		,'01'			,'04'			,'Nome Cliente'        				,'Nome Cliente'        				,'Nome Cliente'        				,'Posicione("SA1",1,xFilial("SA1")+ZAJ->ZAJ_CLIFAT+ZAJ->ZAJ_LJFAT,"A1_NOME")'                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJCLI'		,'5'		,'01'			,''  			,''                    				,''                    				,''                    				,'ZAJ->ZAJ_CLIFAT'                                                                                                                                                                                                                                           			,''                                                                                                                                                                                                                                                          				})

	aAdd(alSXB,	{	'ZAJLJ' 		,'1'		,'01'			,'DB'			,'Lj cli Fat Alteracao'				,'Lj cli Fat Alteracao'				,'Lj cli Fat Alteracao'				,'ZAJ'                                                                                                                                                                                                                                                       			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJLJ' 		,'2'		,'01'			,'01'			,'Num Alt+cli.Fat+lj Fat.'			,'Num Alt+cli.Fat+lj Fat.'			,'Num Alt+cli.Fat+lj Fat.'		,''                                                                                                                                                                                                                                                          			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJLJ' 		,'4'		,'01'			,'01'			,'Num Alt.Ped.'        				,'Num Alt.Ped.'        				,'Num Alt.Ped.'        				,'ZAJ_NUMALT'                                                                                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJLJ' 		,'4'		,'01'			,'02'			,'Cli faturame'        				,'Cli faturame'        				,'Cli faturame'        				,'ZAJ_CLIFAT'                                                                                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJLJ' 		,'4'		,'01'			,'03'			,'Loj faturame'        				,'Loj faturame'        				,'Loj faturame'        				,'ZAJ_LJFAT'                                                                                                                                                                                                                                                 			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJLJ' 		,'4'		,'01'			,'04'			,'Nome Cliente'        				,'Nome Cliente'        				,'Nome Cliente'        				,'Posicione("SA1",1,xFilial("SA1")+ZAJ->ZAJ_CLIFAT+ZAJ->ZAJ_LJFAT,"A1_NOME")'                                                                                                                                                                                			,''                                                                                                                                                                                                                                                          				})
	aAdd(alSXB,	{	'ZAJLJ' 		,'5'		,'01'			,''  			,''                    				,''                    				,''                    				,'ZAJ->ZAJ_LJFAT'                                                                                                                                                                                                                                            			,''                                                                                                                                                                                                                                                          				})

  	For nlI := 1 To Len(alSXB)
   		If nlI != Len(alSXB)
	   		If AllTrim(alSXB[nlI][1]) <> AllTrim(alSXB[(nlI+1)][1])
		  		aAdd(alAlias,{alSXB[nlI][1]})
			EndIf
		Else
			aAdd(alAlias,{alSXB[nlI][1]})
		EndIf
	Next nlI

	dbSelectArea("SXB")
	SXB->(dbSetOrder(1))
	SXB->(dbGoTop())
	For nlI := 1 To Len(alAlias)
		If SXB->(dbSeek(PadR(alAlias[nlI][1],6)))
			While AllTrim(SXB->XB_ALIAS) == AllTrim(alAlias[nlI][1])
				SXB->(RecLock("SXB",.F.))
					SXB->(dbDelete())
				SXB->(MSUnlock())
				SXB->(dbSkip())
			EndDo
		EndIf
		SXB->(dbGoTop())
	Next nlI

	dbSelectArea("SXB")
	SXB->(dbSetOrder(1))
	For nlI := 1 To Len(alSXB)
		If !Empty(alSXB[nlI,1])	
			RecLock("SXB",!(DbSeek(PadR(alSXB[nlI,1],6)+alSXB[nlI,2]+alSXB[nlI,3]+alSXB[nlI,4])))

			For nlJ :=1 To Len(alSXB[nlI])
				If FieldPos(alEstrut[nlJ]) > 0
					FieldPut(FieldPos(alEstrut[nlJ]),alSXB[nlI,nlJ])
				EndIf
			Next nlJ
			MsUnLock()
		EndIf
	Next nlI

	clTexto := "Concluida a atualizacao da SXB" + CRLF

Return clTexto
